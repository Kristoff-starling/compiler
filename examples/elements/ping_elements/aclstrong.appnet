state
{
	@consistency(strong) @combiner(LWW) @persistence(true)
	acl: Map<string, string>
}

fn init() {
	acl.set('test', 'No');
}

fn req(rpc) {
	match (acl.get(rpc.get('body'))) {
		Some(permission) => {
			match (permission) {
				'Yes' => {
					send(rpc, Down);
				}
				'No' => {
					send(err('acl'), Up);
				}
				_ => {
				}
			};
		}
		None => {
			send(rpc, Down);
		}
	};
}

fn resp(rpc) {
    send(rpc, Up);
}
