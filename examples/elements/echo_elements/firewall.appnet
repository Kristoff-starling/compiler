state
{
	firewall: Map<string, string>
}

fn init() {
	firewall.set('test', 'No');
}

fn req(rpc_req) {
	match (firewall.get(rpc_req.get('body'))) {
		Some(permission) => {
			match (permission) {
				'Yes' => {
					send(rpc_req, Down);
				}
				'No' => {
					send(err('blocked by firewall'), Up);
				}
				_ => {
				}
			};
		}
		None => {
			send(rpc_req, Down);
		}
	};
}

fn resp(rpc_resp) {
    send(rpc_resp, Up);
}
